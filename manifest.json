{
    "manifest_version": 3,
    "name": "Chrome Sandbox",
    "version": "1",
    "description": "NICE",
        "background": {
            "service_worker": "background.js"
        },
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "nativeMessaging",
         "declarativeNetRequest"
    ]
}